{
    "scopeName": "source.ist",
    "patterns": [
      {
        "name": "keyword.operator.istari",
          "match": "(?:->|=>|:=|=|\\+|-|\\*|\\/|\\.|,|%|&)"
      },
      {
        "name": "keyword.control.istari",
        "match": "(?<!\\w)(fn|forall|lemma)(?!\\w)"
      },
      {
        "name": "constant.numeric.istari",
        "match": "\\b\\d+\\b"
      },
      {
        "name": "comment.block.istari",
        "begin": "\\(\\*",
        "end": "\\*\\)",
        "patterns": []
      },
      {
        "name": "string.quoted.double.istari",
        "begin": "\"",
        "end": "\"",
        "patterns": []
      },
      {
        "name": "variable.other.istari",
        "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
      }
    ]
  }
  