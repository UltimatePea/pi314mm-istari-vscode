{
    "scopeName": "source.ist",
    "patterns": [
      {
        "name": "keyword.operator.istari",
          "match": "(?:->|=>|:=|=|\\+|-|\\*|\\/|\\.|,|%|&)"
      },
      {
        "name": "keyword.control.istari",
        "match": "(?<!\\w)(fn|forall|iforall|iexists|intersect|union|foralltp|rec|mu|wtype|iset|let|seq|seqt|case|of|fnind|if|then|else)(?!\\w)"
      },
      {
        "name": "keyword.other.istari",
        "match": "(?<!\\w)(lemma|define|defineInd|definerec|openModule|beginModule|endModule|qed|File\\.import|reductions|val)(?!\\w)"
      },
      {
        "name": "constant.language.istari",
        "match": "(?<!\\w)(next|future|unit|void)(?!\\w)"
      },
      {
        "name": "constant.numeric.istari",
        "match": "\\b\\d+\\b"
      },
      {
        "name": "comment.block.istari",
        "begin": "\\(\\*",
        "end": "\\*\\)",
        "patterns": []
      },
      {
        "name": "string.quoted.double.istari",
        "begin": "\"",
        "end": "\"",
        "patterns": []
      },
      {
        "name": "variable.other.istari",
        "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
      }
    ]
  }
  